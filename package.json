    "@reduxjs/toolkit": "^1.8.5",
    "react-redux": "^8.0.2",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "dev": "REACT_APP_ENV=local react-scripts start",
    "start": "REACT_APP_ENV=production react-scripts start",
    "build": "REACT_APP_ENV=production react-scripts build",
    "start-development": "REACT_APP_ENV=development react-scripts start",
    "build-development": "REACT_APP_ENV=development react-scripts build",
    "start-staging": "REACT_APP_ENV=staging react-scripts start",
    "build-staging": "REACT_APP_ENV=staging react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write --ignore-unknown",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
